{
	"name": "MLClusteringKMeans",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "predictors",
			"type": "Variables"
		},
		{
			"name": "noOfClusters",
			"type": "List",
			"options": [ "auto", "manual", "optimized", "robust" ],
			"default": "auto"
		},
		{
			"name": "clusterSize",
			"type": "Integer"
		},
		{
			"name": "noOfIterations",
			"type": "List",
			"options": [ "auto", "manual" ],
			"default": "auto"
		},
		{
			"name": "noOfRandomSets",
			"type": "List",
			"options": [ "auto", "manual" ],
			"default": "auto"
		},
		{
			"name": "nearestNeighboursCount",
			"type": "Integer"
		},
		{
			"name": "optimizedFrom",
			"type": "Integer"
		},
		{
			"name": "optimizedTo",
			"type": "Integer"
		},
		{
			"name": "robustFrom",
			"type": "Integer"
		},
		{
			"name": "robustTo",
			"type": "Integer"
		},
		{
			"name": "iterationsCount",
			"type": "Integer"
		},
		{
			"name": "randomSetCount",
			"type": "Integer"
		},
		{
			"name": "plot2dCluster",
			"type": "Boolean"
		},
		{
			"name": "plot2dClusterCentroids",
			"type": "Boolean"
		},
		{
			"name": "plotCriterionVsClusters",
			"type": "Boolean"
		},
		{
			"name": "plotPCACluster",
			"type": "Boolean"
		},
		{
			"name": "plotPCAClusterSquares",
			"type": "Boolean"
		},
		{
			"name": "tableClusterInformation",
			"type": "Boolean"
		},
		{
			"name": "tableClusterInfoSize",
			"type": "Boolean"
		},
		{
			"name": "tableClusterInfoCentroids",
			"type": "Boolean"
		},
		{
			"name": "tableClusterInfoSumSquares",
			"type": "Boolean"
		},
		{
			"name": "tableClusterInfoTotalSumSquares",
			"type": "Boolean"
		},
		{
			"name": "tableClusterInfoBetweenSumSquares",
			"type": "Boolean"
		},
		{
			"name": "tablePredictions",
			"type": "Boolean"
		},
		{
			"name": "naAction",
			"type": "List",
			"options": [ "deleteListwise", "predict", "roughFix" ],
			"default": "deleteListwise"
		},
		{
			"name": "algorithm",
			"type": "List",
			"options": [ "hartiganWong", "lloyd", "macQueen" ],
			"default": "hartiganWong"
		},
		{
			"name": "criterion",
			"options": [ "silhoutteLength" ],
			"type": "List"
		}
	]
}
